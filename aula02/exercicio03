import java.util.Scanner;

public class exercicio18 {
    public static void main(String[] args) {
        Scanner sc = new Scanner (System.in);    
        float gas, eta, eficiencia;
        
        System.out.printf("Qual a cotação do valor do Etanol?%n");
        eta = sc.nextFloat();
        System.out.printf("Qual a cotação do valor da gasolina?%n");
        gas = sc.nextFloat();

        eficiencia = (gas * 70)/100;
        if(eficiencia > eta){
            System.out.printf("Eh mais eficiente abastecer com etanol!%n");
        } else {
            System.out.printf("Eh mais eficiente abastecer com gasolina!%n");
        }
        sc.close();
    }
}
